{
  "manifest_version": 3,
  "name": "PriceAnalyst",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "http://localhost:4000/*", 
    "*://*.wildberries.ru/*", 
    "*://*.ozon.ru/*",
    "*://www.wildberries.ru/*",
    "*://www.ozon.ru/*"
  ],
  "action": { 
    "default_popup": "popup.html"
  }
}
